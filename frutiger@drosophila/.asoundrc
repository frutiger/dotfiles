pcm.pro {
  type asym
  playback {
    pcm {
      type dmix
      ipc_key 1
      ipc_key_add_uid yes
      slave {
        pcm "hw:2,0"
      }
    }
  }
  capture {
    pcm "hw:2,1"
  }
}

pcm.internal {
  type asym
  playback {
    pcm {
      type dmix
      ipc_key 2
      ipc_key_add_uid yes
      slave {
        pcm "hw:0,0"
      }
    }
  }
  capture {
    pcm "hw:0,0"
  }
}

pcm.jackplug {
  type plug
  slave {
    pcm "jack"
  }
}

pcm {
  type jack
  playback_ports {
    0: system:playback_1
    1: system:playback_2
  }
  capture_ports {
    0: system:capture_1
    1: system:capture_2
  }
}

pcm.pulse {
  type pulse
}

ctl.pulse {
  type pulse
}

pcm.!default plug:internal

